# Local dev format: postgresql://USER:PASSWORD@HOST:PORT/DB (default port 5432)
DATABASE_URL="postgresql://gauntlet:gauntlet@localhost:5432/gauntlet"
ADMIN_TOKEN="change-me"
UPLOADS_DIR="./data/uploads"
RUNS_DIR="/tmp/gauntlet/jobs"
WORKER_POLL_INTERVAL_MS=3000
WORKER_MAX_CONCURRENCY=1
WORKER_WATCHDOG_INTERVAL_MS=2000
MAX_JOB_RUNTIME_MS=5000
WORKER_ENABLED="false"
RUN_UNTRUSTED_CODE="false"
SUBMISSION_RATE_LIMIT_IP=10
SUBMISSION_RATE_LIMIT_USER=20
DOCKER_NODE_IMAGE="node:22-slim"
